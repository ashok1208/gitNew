<apex:page standardController="Customer__c" >
    <apex:form >
        <apex:pageBlock title="My Page">
            <apex:pageBlockSection columns="1" >
                <apex:inputField value="{!Customer__c.name}"/>
                <apex:inputField value="{!Customer__c.Ammount__c}"/>
                <apex:inputField value="{!Customer__c.Country__c}"/>
                <apex:inputField value="{!Customer__c.States__c}"/>
                <apex:inputField value="{!Customer__c.Phone_NO__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="Submit" action="{!save}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>        
    </apex:form>
</apex:page>