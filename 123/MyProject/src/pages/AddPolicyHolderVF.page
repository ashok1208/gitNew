<apex:page standardController="Policy_Holder__c" extensions="AddPolicyHolderCtrl" showHeader="false">
<center>
<apex:form >
<center>
    <apex:pageBlock title="New Policy Holder">
        <apex:pageBlockSection columns="1" >
        
            <apex:inputField value="{!cds.name}"/>
            <apex:inputField value="{!cds.Date_of_Birth__c}"/>
            <apex:inputField value="{!cds.Gender__c}"/>
            <apex:inputField value="{!cds.Phone__c}"/>
            <apex:inputField value="{!cds.Email__c}"/>
            <apex:inputField value="{!cds.Occupation__c}"/>
            <apex:inputField value="{!cds.Income__c}"/>
            <apex:inputField value="{!cds.Address__c}"/>
            <apex:inputField value="{!cds.Agent_ID__c}"/>

            
            
        </apex:pageBlockSection>
    
    </apex:pageBlock>
    
    
      <apex:commandButton value="SAVE" action="{!save}"/>
      <apex:commandButton value="Save & New" action="{!SaveandNew}"/>
      <apex:commandButton value="Cancel" action="{!cancel}"/>
      
</center>
</apex:form> 
</center>
</apex:page>