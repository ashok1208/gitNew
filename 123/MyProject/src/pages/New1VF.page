<apex:page standardController="Loan__c" showHeader="false" >
    <apex:form >
        <apex:pageblock title="Form" >
            <apex:pageBlockSection columns="1" >                
                <apex:inputField value="{!Loan__c.Name}"/>
                <apex:inputField value="{!Loan__c.Country__c}"/>
                <apex:inputField value="{!Loan__c.Gender__c}"/>
                <apex:inputField value="{!Loan__c.Account_Type__c}"/>
                <apex:inputField value="{!Loan__c.AccountType__c}"/>
                
            </apex:pageBlockSection>            
             
        </apex:pageblock>
        <center><apex:commandButton value="Submit" action="{!Save}"/></center>
    </apex:form>
</apex:page>